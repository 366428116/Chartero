{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "inlineSourceMap": true,
        "isolatedModules": false,
        "forceConsistentCasingInFileNames": true
    }
}