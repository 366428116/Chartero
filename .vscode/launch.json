    "version": "0.2.0",
    "configurations": [
        {
            "name": "Vue",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "http://localhost:5173/",
            "pathMappings": [
                {
                    "url": "http://localhost:5173/@fs/F:/zotero%20add-on/Chartero/src",
                    "path": "${workspaceFolder}/src"
                },
                {
                    "url": "http://localhost:5173",
                    "path": "${workspaceFolder}/src/vue/test"
                },
                {
                    "url": "http://localhost:5173/@fs/F:/zotero%20add-on/Chartero/",
                    "path": "${workspaceFolder}"
                }
            ],
            "preLaunchTask": "npm: dev"
        }
    ]
}