    "version": "0.2.0",
    "configurations": [
        {
            "name": "Vue",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "http://localhost:5173/",
            "firefoxExecutable": "F:\\Firefox\\firefox.exe",
            "pathMappings": [
                {
                    "url": "http://localhost:5173/@fs/E:/zotero/chartero/src",
                    "path": "${workspaceFolder}/src"
                },
                {
                    "url": "http://localhost:5173/@fs/E:/zotero/chartero/node_modules",
                    "path": "${workspaceFolder}/node_modules"
                },
                {
                    "url": "http://localhost:5173/@vite",
                    "path": "${workspaceFolder}/src/vue/test/vite/packages/vite"
                },
                {
                    "url": "http://localhost:5173",
                    "path": "${workspaceFolder}/src/vue/test"
                },
                {
                    "url": "http://localhost:5173/@fs/F:/zotero%20add-on/Chartero/",
                    "path": "${workspaceFolder}"
                }
            ],
            "preLaunchTask": "npm: dev"
        }
    ]
}